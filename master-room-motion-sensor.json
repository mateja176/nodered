    "in": [{ "x": 40, "y": 80, "wires": [{ "id": "ccf036ee.a92cc8" }] }],
    "out": [
      { "x": 360, "y": 80, "wires": [{ "id": "ccf036ee.a92cc8", "port": 0 }] }
    ],
    "env": [
      { "name": "lowerBound", "type": "num", "value": "20" },
      { "name": "upperBound", "type": "str", "value": "7" }
    ],
    "meta": {},
    "color": "#DDAA99"
  },
  {
    "id": "ccf036ee.a92cc8",
    "type": "function",
    "z": "adc384dc.d4f578",
    "name": "filter",
    "func": "const hours = msg.payload;\nconst lowerBound = env.get('lowerBound');\nconst upperBound = env.get('upperBound');\n\nconst isContinuous = lowerBound < upperBound;\n\nconst isBetween = hours >= lowerBound || hours <= upperBound;\n\nif (\n  isContinuous\n    ? isBetween\n    : (hours >= lowerBound && hours <= 23) || (hours >= 0 && hours <= upperBound)\n) {\n  return msg;\n}\n",
    "outputs": 1,
    "noerr": 0,
    "initialize": "",
    "finalize": "",
    "libs": [],
    "x": 170,
    "y": 80,
    "wires": [[]]
  },

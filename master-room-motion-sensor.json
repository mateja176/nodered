    "z": "66bdcee4.fe0dc",
    "name": "turn_on_temp",
    "func": "const timeout = flow.get('timeout')\nif (timeout) {\n    clearTimeout(timeout)\n}\nconst newTimeout = setTimeout(() => {\n    node.send({ payload: 0 })\n}, [1000 * 60 * env.get('TIMEOUT_MINUTES')]) \nflow.set('timeout', newTimeout)\nreturn {\n    payload: 1\n};",
